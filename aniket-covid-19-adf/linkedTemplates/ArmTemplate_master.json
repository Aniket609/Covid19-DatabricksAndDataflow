{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aniket-covid-19-adf"
		},
		"AzureDatabricksCovid19_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksCovid19'"
		},
		"AzureSqlDatabaseCovid19_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseCovid19'"
		},
		"Covid19ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Covid19ADLSGen2'"
		},
		"AzureDatabricksCovid19_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0718-044128-3t5h1yn5"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://aniketdatabrickskeyvault.vault.azure.net/"
		},
		"AzureSqlDatabaseCovid19_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "covid19srvr.database.windows.net"
		},
		"AzureSqlDatabaseCovid19_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid_reporting"
		},
		"AzureSqlDatabaseCovid19_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "aniketcovid"
		},
		"Covid19ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aniketcovid19.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aniket-covid-19-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksCovid19_accessToken": {
						"value": "[parameters('AzureDatabricksCovid19_accessToken')]"
					},
					"AzureSqlDatabaseCovid19_password": {
						"value": "[parameters('AzureSqlDatabaseCovid19_password')]"
					},
					"Covid19ADLSGen2_accountKey": {
						"value": "[parameters('Covid19ADLSGen2_accountKey')]"
					},
					"AzureDatabricksCovid19_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksCovid19_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabaseCovid19_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseCovid19_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseCovid19_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseCovid19_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseCovid19_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseCovid19_properties_typeProperties_userName')]"
					},
					"Covid19ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('Covid19ADLSGen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}